{
    "name": "kaiyum2012/socialite-auth",
    "description": "Plug and use of Laravel Socialite package for Laravel App",
    "keywords": [
        "kaiyum2012",
        "socialite-auth",
        "Laravel",
        "Socialite"
    ],
    "homepage": "https://github.com/kaiyum2012/socialite-auth",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Abdulkaiyum Shaikh",
            "email": "abdulkaiyum.creatrix@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "laravel/socialite": "^5.2"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Kaiyum2012\\SocialiteAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kaiyum2012\\SocialiteAuth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kaiyum2012\\SocialiteAuth\\SocialiteAuthServiceProvider"
            ],
            "aliases": {
                "SocialiteAuth": "Kaiyum2012\\SocialiteAuth\\SocialiteAuthFacade"
            }
        }
    }
}
